{
  "name": "@metalsmith/permalinks",
  "description": "A metalsmith plugin for permalinks.",
  "keywords": [
    "permalinks",
    "metalsmith",
    "metalsmith-plugin",
    "static-site"
  ],
  "homepage": "https://github.com/metalsmith/permalinks#readme",
  "bugs": {
    "url": "https://github.com/metalsmith/permalinks/issues"
  },
  "author": "Ian Storm Taylor (https://ianstormtaylor.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/metalsmith/permalinks.git"
  },
  "version": "2.4.0",
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "changelog": "auto-changelog -u --starting-date 2021-05-01 --commit-limit false --ignore-commit-pattern '(dev|Release|Merge)'",
    "coverage": "nyc report --reporter=text-lcov > ./coverage.info",
    "coveralls": "npm run coverage && cat ./coverage.info | coveralls",
    "format": "prettier --write \"**/*.{yml,md,js,json}\"",
    "lint": "eslint --cache --fix-dry-run \"**/*.js\"",
    "dev": "nodemon --exec \"npm run lint && npm test\"",
    "test": "nyc mocha",
    "release": "release-it ."
  },
  "dependencies": {
    "debug": "^4.3.3",
    "moment": "^2.29.1",
    "regexparam": "^2.0.0",
    "slugify": "^1.6.5"
  },
  "devDependencies": {
    "assert-dir-equal": "^1.1.0",
    "auto-changelog": "^2.3.0",
    "coveralls": "^3.1.1",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-recommended": "^4.1.0",
    "metalsmith": "^2.3.0",
    "mocha": "^7.2.0",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "prettier": "^2.5.1",
    "release-it": "^14.12.4",
    "transliteration": "^2.2.0"
  },
  "peerDependencies": {
    "metalsmith": "^2.3.0"
  },
  "files": [
    "lib",
    "CHANGELOG.md"
  ],
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "engines": {
    "node": ">=8"
  },
  "publishConfig": {
    "access": "public"
  }
}
